        <mxCell id="ykjzTQmH_xofyIeEzOZv-7" value="2.用户画像" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=17;" parent="1" vertex="1">
